{
  "name": "react-boilerplate",
  "version": "0.1.0",
  "private": true,
  "author": {
    "name": "abhishek chotaliya"
  },
  "dependencies": {
    "axios": "^0.21.1",
    "env-cmd": "^10.1.0",
    "node-sass": "^5.0.0",
    "prop-types": "^15.7.2",
    "pupa": "^2.1.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-router-dom": "^5.2.0",
    "react-scripts": "4.0.3",
    "web-vitals": "^1.0.1"
  },
  "scripts": {
    "start": "react-scripts start",
    "build:testing": "env-cmd -f ./.env.test react-scripts build",
    "build:staging": "env-cmd -f ./.env.stage react-scripts build",
    "build:production": "env-cmd -f .env.production npm run-script build",
    "eject": "react-scripts eject",
    "format_check": "eslint src/**/*.{js,jsx}",
    "format_fix": "npx eslint src/**/*.{js,jsx} --fix"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "eslint": "^7.27.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-import-resolver-webpack": "^0.13.1",
    "eslint-plugin-import": "^2.23.3",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-prettier": "^3.4.0",
    "eslint-plugin-react": "^7.23.2",
    "eslint-plugin-react-hooks": "^4.2.0",
    "prettier": "^2.3.0"
  }
}
